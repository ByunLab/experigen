<%
var screen = exp.getCurrentScreen();
%>

<div class="trialpartWrapper" id="part1">
	<%= screen.frame.text1 %>
	<B><I><%= (screen.finalConsonant=="l") ? screen.item : screen.item_j %></I></B>
	<%= screen.frame.text2 %>
	<%= screen.makeSoundButton(screen["file_" + screen.finalConsonant]) %>
</div>


<div class="trialpartWrapper" id="part3">
	<%= screen.makeScale({direction: exp.getCurrentScreen().HORIZONTAL, buttons: ["1","2","3","4","5","6","7"], leftlabels:['Good','','','','','',''], rightlabels: ['','','','','','','Bad']})  %>
</div>


<div class="trialpartWrapper" id="part2">
	<%= screen.frame.text1 %>
	<B><I><%= (screen.finalConsonant=="l") ? screen.item : screen.item_j %></I></B>
	<%= screen.frame.text2 %>
	<%= screen.makeSoundButton(screen["file_pl_" + screen.finalConsonant]) %>
</div>


